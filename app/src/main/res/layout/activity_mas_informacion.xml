<?xml version="1.0" encoding="utf-8"?>
<android.support.constraint.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    tools:context="com.example.solvo.solvo.MasInformacion">

    <TextView
        android:id="@+id/textView8"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginBottom="84dp"
        android:text="MÁS INFORMACIÓN"
        android:textAlignment="center"
        android:textColor="@color/common_google_signin_btn_text_dark_focused"
        android:textStyle="bold"
        app:layout_constraintBottom_toTopOf="@+id/ID_Marcador"
        tools:layout_editor_absoluteX="128dp" />

    <TextView
        android:id="@+id/ID_Marcador"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginBottom="23dp"
        android:layout_marginTop="83dp"
        android:text="INFO MARCADOR"
        app:layout_constraintBottom_toTopOf="@+id/NomMrk"
        app:layout_constraintTop_toBottomOf="@+id/textView8"
        tools:layout_editor_absoluteX="138dp" />

    <TextView
        android:id="@+id/NomMrk"
        android:layout_width="104dp"
        android:layout_height="25dp"
        android:layout_marginTop="32dp"
        android:text="NOMBRE MRK"
        app:layout_constraintTop_toBottomOf="@+id/ID_Marcador"
        tools:layout_editor_absoluteX="140dp" />
</android.support.constraint.ConstraintLayout>
